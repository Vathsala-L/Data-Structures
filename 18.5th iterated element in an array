#include <stdio.h>    
int main()    
{    
    int arr[] = {1, 2, 3, 4, 2, 7, 8, 8, 3};       
    int length = sizeof(arr)/sizeof(arr[0]);     
    for(int i = 0; i < length; i++) {       
            if(i==4)    
                printf("%d\n", arr[i]);    
        }    
    }    
    return 0;    
}  

#include <stdio.h>
int main()
{
    int N= 5,n, i, j;
    int a[5] = { 1, 2, 3, 4, 5 };
    int b[5] = { 6, 7, 8, 9, 10 };
    n=N+N;
    int c[n];
    for (i = 0; i <N; i++) {
        c[i] = a[i];
    }
    for (i = 0, j =N;j <n && i <N; i++, j++) {
        c[j] = b[i];
    }
    for (i = 0; i < arr_resultsize; i++) {
        printf("%d\n", c[i]);
    }
    return 0;
}
